<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="jQueryScript_1 Automation Testing">

    <parameter name="url" value="https://www.jqueryscript.net/demo/CRUD-Data-Grid-Plugin-jQuery-Quickgrid/"/>
    
    <test name="Run on Firefox browser">
        <parameter name="browser" value="Firefox"/>
        <classes>
            <class name="com.jqueryscript.Level_11_1_Data_Table"/>
        </classes>
    </test>

    <test name="Run on Chrome browser">
        <parameter name="browser" value="Chrome"/>
        <classes>
            <class name="com.jqueryscript.Level_11_1_Data_Table"/>
        </classes>
    </test>

    <test name="Run on Edge browser">
        <parameter name="browser" value="Edge"/>
        <classes>
            <class name="com.jqueryscript.Level_11_1_Data_Table"/>
        </classes>
    </test>

</suite>